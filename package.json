{
  "name": "react-starter",
  "version": "0.1.5",
  "repository": "https://gitlab.zengenti.com/starter-projects/react-starter",
  "license": "None",
  "description": "",
  "main": "index.js",
  "config": {
    "startup": ""
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.dev.js",
    "build": "rimraf ./dist && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js && npm run build:startup",
    "build:assets": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.assets.js",
    "build:ci": "npm run build && npm run build:assets",
    "build:startup": "buildstartup",
    "server": "node --max-http-header-size=800000 dist/start.js",
    "dev:server": "cross-env NODE_ENV=test NODE_OPTIONS=--max-http-header-size=80000 babel-node -x .ts,.tsx,.js,.jsx --inspect=9229 src/server/debug.ts",
    "debug:server": "nodemon --delay 500ms --watch \"src/**\" --ext \"ts,tsx,js,jsx,json\" --signal SIGTERM --exec \"npx kill-port 3001 && npm run dev:server\"",
    "build:server": "npm run build && npm run server",
    "build:dev:server": "npm run build && npm run dev:server",
    "analyze": "rimraf ./dist && cross-env NODE_ENV=production ANALYZE=TRUE webpack --config webpack/webpack.config.prod.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest  -c ./config/jest.config.js --detectOpenHandles --forceExit",
    "test:watch": "jest --config=./config/jest.config.js --watch",
    "test:coverage": "jest --config=./config/jest.config.js --coverage",
    "prebuild:storybook": "npm run test:generate-output",
    "storybook": "storybook dev -p 6006",
    "storybook-ci": "storybook dev --ci -p 6006",
    "storybook-static": "storybook build -c .storybook -o .out",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "author": "http://Zengenti.com",
